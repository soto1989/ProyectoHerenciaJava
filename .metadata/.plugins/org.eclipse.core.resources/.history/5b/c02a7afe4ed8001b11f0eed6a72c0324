package Logica;

public class Profesor extends Persona {
	
	
	private int matricula;
	private String curso;
	
	
	public Profesor() {
		
	}


	public Profesor(int matricula, String curso, String nombre, String apellido, int edad, String telefono) {
		super(nombre,apellido,edad,telefono);
		this.matricula = matricula;
		this.curso = curso;
	}


	public int getMatricula() {
		return matricula;
	}


	public void setMatricula(int matricula) {
		this.matricula = matricula;
	}


	public String getCurso() {
		return curso;
	}


	public void setCurso(String curso) {
		this.curso = curso;
	}


	@Override
	public String toString() {
		return "Profesor [matricula=" + matricula + ", curso=" + curso + ", nombre=" + nombre + ", apellido=" + apellido
				+ ", edad=" + edad + ", telefono=" + telefono + ", getNombre()=" + getNombre() + ", getApellido()="
				+ getApellido() + ", getEdad()=" + getEdad() + ", getTelefono()=" + getTelefono() + ", toString()="
				+ super.toString() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + "]";
	}


	


	
	



	
	
	
	

}
